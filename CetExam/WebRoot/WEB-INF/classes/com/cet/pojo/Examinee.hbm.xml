<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.cet.pojo.Examinee" table="examinee" catalog="cetexam">
        <id name="candidate" type="java.lang.String">
            <column name="candidate" length="20" />
          
        </id>
        <property name="name" type="java.lang.String">
            <column name="name" length="8" not-null="true" />
        </property>
        <property name="password" type="java.lang.String">
            <column name="password" length="20" not-null="true" />
        </property>
        <property name="sex" type="java.lang.String">
            <column name="sex" length="1" />
        </property>
        <property name="school" type="java.lang.String">
            <column name="school" length="20" not-null="true" />
        </property>
        <property name="image" type="java.lang.String">
            <column name="image" length="100" not-null="true" />
        </property>
        <property name="email" type="java.lang.String">
            <column name="email" length="20" />
        </property>
    </class>
</hibernate-mapping>
