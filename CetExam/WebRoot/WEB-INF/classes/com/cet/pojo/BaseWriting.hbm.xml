<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.cet.pojo.BaseWriting" table="base_writing" catalog="cetexam">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
        </id>
        <property name="title" type="java.lang.String">
            <column name="title" length="65535" not-null="true" />
        </property>
        <property name="imgurl" type="java.lang.String">
            <column name="imgurl" length="100" />
        </property>
        <property name="difficulty" type="java.lang.Float">
            <column name="difficulty" precision="4" scale="3" />
        </property>
        <property name="access" type="java.lang.Float">
            <column name="access" precision="7" scale="1" />
        </property>
        <property name="total" type="java.lang.Integer">
            <column name="total" />
        </property>
    </class>
</hibernate-mapping>
